package ua.group42.taskmanager.common.control;

/**
 *
 * @author Group42
 */
public class InternalControllerException extends RuntimeException {

    /**
     * Creates a new instance of <code>InternalControllerException</code> without detail message.
     */
    public InternalControllerException() {
    }

    /**
     * Constructs an instance of <code>InternalControllerException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public InternalControllerException(String msg) {
        super(msg);
    }
    
    /**
     * Constructs an instance of <code>InternalControllerException</code> with the specified detail message and cause.
     * @param msg the detail message.
     * @param cause the error caused this exception
     */
    public InternalControllerException(String msg, Throwable cause) {
        super(msg, cause);
    }
}
